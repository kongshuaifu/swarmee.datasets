{
  "template": "nrl-results-2-4-2017",
  "settings": {
    "number_of_shards": 1,
    "number_of_replicas": 1,
    "index.codec": "best_compression",
    "index.refresh_interval" : "3s"
  },
    "mappings": {
      "nrl-results": {
        "properties": {
          "urbanArea": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256
              }
            }
          },
          "population": {
            "type": "long"
          }
        }
      }
    }
    }

